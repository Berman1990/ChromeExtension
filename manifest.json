{
  "manifest_version": 2,

  "name": "Sisense Direct",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "sisense.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "sisense.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
  {
    "matches": ["http://www.gmail.com/*", "https://mail.google.com/*"],
    "css": ["mystyles.css"],
    "js": ["myScript.js"]
  }
]
}
